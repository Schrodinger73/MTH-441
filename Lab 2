library(readxl)

# P1
data.File1 <- read_excel("Electricity_Data.xlsx")
model1 <- lm(Y ~ X, data = data.File1)
model1$coefficients

# P2
data.File2 <- read_excel("TimeDeliveryData.xlsx")
model2 <- lm(Y ~ X1 + X2, data = data.File2)
model2$coefficients

# P3
sum(model1$residuals^2)/(length(data.File1$Y) - 2)
sum(model2$residuals^2)/(length(data.File2$Y) - 3)

